[
  {
    "host": "*://*.crunchyroll.com/*",
    "auto": false,
    "allFrames": false,
    "disabled": true
  },
  {
    "host": "file:///*test*.html",
    "auto": true,
    "allFrames": false,
    "parse": "body"
  },
  {
    "host": "<all_urls>",
    "auto": false,
    "allFrames": false,
    "parse": "body"
  },
  {
    "host": "*://*.nhk.or.jp/*",
    "auto": true,
    "allFrames": false,
    "parse": "article"
  },
  {
    "host": [
      "*://anacreondjt.gitlab.io/texthooker.html",
      "*://learnjapanese.moe/texthooker.html",
      "*://kamwithk.github.io/exSTATic/tracker.html",
      "*://renji-xd.github.io/texthooker-ui/"
    ],
    "auto": true,
    "allFrames": false,
    "parseVisibleObserver": true,
    "addedObserver": {
      "notifyFor": ".textline, .line_box, .sentence-entry, .my-2.cursor-pointer, p",
      "observeFrom": ["#textlog, #entry_holder, main", "body"],
      "config": {
        "childList": true,
        "subtree": true
      }
    }
  },
  {
    "host": "*://app.readwok.com/*",
    "auto": true,
    "allFrames": false,
    "parseVisibleObserver": true,
    "addedObserver": {
      "notifyFor": "div[class*=\"styles_paragraph_\"], div[class*=\"styles_reader_\"]",
      "observeFrom": "body",
      "config": {
        "childList": true,
        "subtree": true
      }
    }
  },
  {
    "host": "*://reader.ttsu.app/*",
    "auto": true,
    "allFrames": false,
    "parseVisibleObserver": {
      "exclude": "[data-ttu-spoiler-img]"
    },
    "addedObserver": {
      "notifyFor": "div[id*=\"ttu-id\"], .ttu-book-html-wrapper, div[id*=\"ttu-p-toc\"], div[id*=\"ttu-p\"]",
      "observeFrom": "body",
      "config": {
        "childList": true,
        "subtree": true
      }
    }
  },
  {
    "host": ["*://*.youtube.com/*", "*://*.youtu.be/*"],
    "auto": true,
    "allFrames": false,
    "parseVisibleObserver": true,
    "addedObserver": {
      "notifyFor": "ytd-comment-view-model",
      "observeFrom": "body",
      "config": {
        "childList": true,
        "subtree": true
      }
    }
  },
  {
    "host": "*://reader.mokuro.app/*",
    "auto": true,
    "allFrames": true,
    "custom": "MokuroParser",
    "parseVisibleObserver": true,
    "addedObserver": {
      "notifyFor": "#manga-panel",
      "observeFrom": "body",
      "config": {
        "childList": true,
        "subtree": true
      }
    }
  },
  {
    "host": "file:///*mokuro*.html",
    "auto": true,
    "allFrames": true,
    "custom": "MokuroLegacyParser",
    "parseVisibleObserver": true
  },
  {
    "host": ["*://ja.wikipedia.org/*", "*://ja.m.wikipedia.org/*"],
    "auto": true,
    "allFrames": false,
    "parseVisibleObserver": {
      "exclude": ".p-lang-btn, .vector-menu-heading-label, .vector-toc-toggle, .vector-page-toolbar, .mw-editsection, sup.reference"
    },
    "addedObserver": {
      "notifyFor": "#firstHeading, #mw-content-text .mw-parser-output > *, .mwe-popups-extract > *",
      "observeFrom": "body",
      "config": {
        "childList": true,
        "subtree": true
      }
    }
  },
  {
    "host": "<all_urls>",
    "auto": true,
    "allFrames": true,
    "css": ".asbplayer-subtitles-container-bottom { z-index: 2147483646 }",
    "addedObserver": {
      "notifyFor": ".asbplayer-offscreen",
      "observeFrom": "body",
      "config": {
        "childList": true,
        "subtree": true
      }
    }
  }
]
